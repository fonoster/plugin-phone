{
  "name": "@fonos/plugin-phone",
  "version": "0.1.1",
  "author": "Pedro Sanders <psanders@fonoster.com>",
  "scripts": {
    "build": "tsc && webpack --progress --config webpack.config.cjs",
    "prepack": "rimraf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "postpack": "rimraf oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  },
  "dependencies": {
    "@oclif/command": "^1.8.0",
    "@oclif/config": "^1.17.0",
    "@oclif/errors": "^1.3.4",
    "cli-ux": "^5.5.1",
    "easy-table": "^1.1.1",
    "express": "^4.17.1",
    "get-stdin": "^9.0.0",
    "get-stdin-with-tty": "^6.0.0",
    "tslib": "^1.14.1",
    "wphone": "github:fonoster/wphone"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.26.0",
    "@oclif/plugin-help": "^3.2.2",
    "@oclif/test": "^1.2.8",
    "@types/chai": "^4.2.18",
    "@types/express": "^4.17.13",
    "@types/mocha": "^5.2.7",
    "@types/node": "^10.17.60",
    "chai": "^4.3.4",
    "dotenv": "^10.0.0",
    "eslint": "^5.16.0",
    "eslint-config-oclif": "^3.1.0",
    "eslint-config-oclif-typescript": "^0.1.0",
    "globby": "^10.0.2",
    "mocha": "^5.2.0",
    "nyc": "^14.1.1",
    "rimraf": "^3.0.2",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.10",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.12"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "files": [
    "/public",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "bugs": "https://github.com/fonoster/fonos-plugin-phone/issues",
  "repository": "https://github.com/fonoster/fonos-plugin-phone/issues",
  "homepage": "https://github.com/fonoster/fonos",
  "keywords": [
    "oclif-plugin"
  ],
  "license": "MIT",
  "oclif": {
    "commands": "./dist/commands",
    "bin": "oclif-example",
    "devPlugins": [
      "@oclif/plugin-help"
    ],
    "topics": {
      "phone": {
        "description": "test/simulator phone (experimental)"
      }
    }
  }
}
